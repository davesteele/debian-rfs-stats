#!/usr/bin/python

import os
import re

basedir = "./tmp"

for fl in os.listdir(basedir):
    ffl = os.path.join( basedir, fl )
    if not re.search( "Bug is archived. ", open( ffl, 'r' ).read() ) \
          and 'cache' in ffl:
        print "Deleting %s" % ffl
        os.unlink( ffl )

thelist = os.path.join( basedir, "rfslist.cache" )
if os.access( thelist, os.R_OK ):
    print "Deleting", thelist
    os.unlink( thelist )


